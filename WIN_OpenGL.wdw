        code : |1+
         
         // Variables
         clSphere      is CSphere dynamic
         clTexturedCube is CTexturedCube dynamic
         clCone        is CCone dynamic
         clCylinder    is CCylinder dynamic
         
         // Erase all the previous objects
         clOpenGL:DeleteFigure()
         
         // The scene moves away from the camera
         clOpenGL:m_ZTransCamera=-11.0
         
         // Head
         clSphere = new CSphere
         clSphere:SetParam(1.0)
         clSphere:SetTranslation(0.0, 1.8, 0.0)
         clSphere:SetColor(RGB(248,218,186))
         clOpenGL:AddFigure(clSphere)
         
         // Hat
         clCone = new CCone
         clCone:SetParam(0.7,1.5)
         clCone:SetRotation(-90.0,0.0,0.0)
         clCone:SetTranslation(0.0, 2.5, 0.0)
         clCone:SetColor(RGB(255,144,54))
         clOpenGL:AddFigure(clCone)
         
         // Left eye
         clSphere = new CSphere
         clSphere:SetParam(0.1)
         clSphere:SetTranslation(0.4, 2.1, 0.8)
         clSphere:SetColor(RGB(15,0,0))
         clOpenGL:AddFigure(clSphere)
         
         // Right eye
         clSphere = new CSphere
         clSphere:SetParam(0.1)
         clSphere:SetTranslation(-0.4, 2.1, 0.8)
         clSphere:SetColor(RGB(0,0,0))
         clOpenGL:AddFigure(clSphere)
         
         // Nose
         clCylinder = new CCylinder
         clCylinder:SetParam(0.2,0.2,1.5)
         clCylinder:SetRotation(0.0,0.0,0.0)
         clCylinder:SetTranslation(0.0, 1.8, 0.0)
         clCylinder:SetColor(RGB(231,149,138))
         clOpenGL:AddFigure(clCylinder)
         
         // End of the nose
         clSphere = new CSphere
         clSphere:SetParam(0.2)
         clSphere:SetTranslation(0.0, 1.8, 1.5)
         clSphere:SetColor(RGB(231,149,138))
         clOpenGL:AddFigure(clSphere)
         
         // Neck
         clCylinder = new CCylinder
         clCylinder:SetParam(0.3,0.3,0.7)
         clCylinder:SetRotation(90.0,0.0,0.0)
         clCylinder:SetTranslation(0.0, 1.1, 0.0)
         clCylinder:SetColor(RGB(247,224,199))
         clOpenGL:AddFigure(clCylinder)
         
         // Left arm
         clCylinder = new CCylinder
         clCylinder:SetParam(0.3,0.3,2.3)
         clCylinder:SetRotation(0.0,-80.0,0.0)
         clCylinder:SetTranslation(0.0, 0.4, 0.0)
         clCylinder:SetColor(RGB(248,218,186))
         clOpenGL:AddFigure(clCylinder)
         
         // Left hand
         clSphere = new CSphere
         clSphere:SetParam(0.3)
         clSphere:SetTranslation(2.15, 0.4, 0.38)
         clSphere:SetColor(RGB(247,224,199))
         clOpenGL:AddFigure(clSphere)
         
         // Right arm
         clCylinder = new CCylinder
         clCylinder:ClearMatrice()
         clCylinder:SetParam(0.3,0.3,2.3)
         clCylinder:SetRotation(0.0,80.0,0.0)
         clCylinder:SetTranslation(0.0, 0.4, 0.0)
         clCylinder:SetColor(RGB(248,218,186))
         clOpenGL:AddFigure(clCylinder)
         
         // Right hand
         clSphere = new CSphere
         clSphere:SetParam(0.3)
         clSphere:SetTranslation(-2.15, 0.4, 0.38)
         clSphere:SetColor(RGB(247,224,199))
         clOpenGL:AddFigure(clSphere)
         
         // Right leg
         clCylinder = new CCylinder
         clCylinder:ClearMatrice()
         clCylinder:SetParam(0.3,0.3,2.3)
         clCylinder:SetRotation(90.0,7.0,0.0)
         clCylinder:SetTranslation(0.5, -0.9, 0.0)
         clCylinder:SetColor(RGB(73,147,186))
         clOpenGL:AddFigure(clCylinder)
         
         // Left leg
         clCylinder = new CCylinder
         clCylinder:ClearMatrice()
         clCylinder:SetParam(0.3,0.3,2.3)
         clCylinder:SetRotation(90.0,-7.0,0.0)
         clCylinder:SetTranslation(-0.5,-0.9, 0.0)
         clCylinder:SetColor(RGB(73,147,186))
         clOpenGL:AddFigure(clCylinder)
         
         // Body
         clTexturedCube = new CTexturedCube
         clTexturedCube:InitTexture(Image2..Name)
         clTexturedCube:SetTranslation(0.0, -0.3, 0.0)
         clOpenGL:AddFigure(clTexturedCube)
         
         
         clOpenGL:Draw()
         
